    "eslint-plugin-react-refresh": "^0.4.5",
    "gh-pages": "^6.2.0",
    "jsdom": "^25.0.1",
    "postcss": "^8.4.49",
    "supertest": "^7.0.0",
    "tailwindcss": "^3.4.15",
    "vite": "^5.1.4",
    "vitest": "^2.1.4"
